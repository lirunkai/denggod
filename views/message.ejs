<%- include('header') %>

<% docs.forEach(function(doc){%>
  <div class="weui-cells">
    <div class="weui-cell">
      <p style="text-align:center"><%= doc.username%></p>
    </div>
    <div class="weui-cell">
        <a class="weui-btn weui-btn_plain-primary" href="/message/<%= doc.openid %>">
          查看订单
        </a>
        <a class="weui-btn weui-btn_warn" href="/delete/user/<%= doc.openid %>">
          删除用户
        </a>
    </div>
  </div>
<% }) %>

<%- include('footer') %>
