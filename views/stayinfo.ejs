<%- include('header') %>
<div class="weui-tab">
  <div class="weui-navbar nav">
      <div class="weui-navbar__item">
        <a href="/home">
          全部
        </a>
      </div>
      <div class="weui-navbar__item">
        <a href="/home/proceed">
          进行中
        </a>
      </div>
      <div class="weui-navbar__item nav_active">
        <a href="/home/stay">
          待还款
        </a>
      </div>
      <div class="weui-navbar__item">
        <a href="/home/settle">
          已结清
        </a>
      </div>
      <div class="weui-navbar__item">
        <a href="/home/lose">
          已失效
        </a>
      </div>
  </div>
  <% docs.forEach(function(doc){ %>
    <div class="weui-tab__panel">
      <div class="weui-cells">
        <div class="weui-cell">
          <div class="weui-cell__bd">
            <p>提交订单时间</p>
          </div>
          <div class="weui-cell__ft"><%= doc.infoCreateTime%></div>
        </div>
        <div class="weui-cell">
          <div class="weui-cell__bd">
            <p>订单号</p>
          </div>
          <div class="weui-cell__ft">
            <%= doc.infoShopNum %>
          </div>
        </div>
        <div class="weui-cell">
          <div class="weui-cell__bd">
            <p>借款人</p>
          </div>
          <div class="weui-cell__ft"><%= doc.infoname%></div>
        </div>
        <a class="weui-cell weui-cell_access" href="/showinfo/<%= doc.infoShopNum %>">
          <div class="weui-cell__bd">
            <p>订单状态</p>
          </div>
          <div class="weui-cell__ft"><%= doc.infoState %></div>
        </a>
      </div>
    </div>
  <% }) %>
</div>
<%- include('footer') %>
